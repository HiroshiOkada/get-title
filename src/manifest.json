{
    "manifest_version": 2,

    "name": "Get Title",
    "description": "Easy to access page title.",
    "version": "1.0",

    "permissions": [
        "activeTab",
        "tabs",
        "clipboardWrite",
        "storage"
    ],

    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        },
        "default_title": "Get Title",
        "default_popup": "popup.html"
    },

    "options_page": "options.html"
}

